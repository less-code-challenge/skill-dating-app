{
  "name": "skill-dating-app-backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build && npm run serve",
    "package": "npm run build && npm run install-node-modules-in-dist",
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix",
    "install-node-modules-in-dist": "ts-node scripts/install-node-modules-in-dist.ts",
    "test": "jest --forceExit --coverage --verbose",
    "test:ci": "jest --forceExit",
    "serve": "node dist/index-local.js",
    "serve-debug": "nodemon --inspect dist/index-local.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/index-local.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "create-tables-local": "ts-node scripts/create-tables-local.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.1",
    "aws-sdk": "^2.722.0",
    "express": "^4.17.1",
    "nanoid": "^3.1.12",
    "serverless-http": "^2.5.0"
  },
  "devDependencies": {
    "@types/concurrently": "^5.2.1",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/js-yaml": "^3.12.5",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.0.27",
    "@types/shelljs": "^0.8.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "jest": "^26.2.1",
    "js-yaml": "^3.14.0",
    "nodemon": "^2.0.4",
    "shelljs": "^0.8.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "author": "",
  "license": "Apache-2.0"
}
